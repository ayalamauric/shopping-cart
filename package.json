{
  "name": "@ecomplus/shopping-cart",
  "version": "0.6.1-alpha.0",
  "description": "Simple vanilla JS library to handle shopping cart object",
  "main": "dist/shopping-cart.min.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "serve": "webpack-dev-server",
    "doc": "rm -rf ./docs/*; jsdoc ./src -r ./README.md -d ./docs -t ./node_modules/docdash",
    "release": "rm -rf ./dist; npm run build && standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomclub/shopping-cart.git"
  },
  "keywords": [
    "ecomplus",
    "ecommerce",
    "storefront",
    "cart"
  ],
  "author": "E-Com Club",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/shopping-cart/issues"
  },
  "homepage": "https://github.com/ecomclub/shopping-cart#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@ecomplus/client": "^1.1.2",
    "@ecomplus/utils": "^1.0.1",
    "core-js": "^3.2.1",
    "eventemitter3": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-loader": "^8.0.6",
    "docdash": "^1.1.1",
    "husky": "^3.0.4",
    "jsdoc": "^3.6.3",
    "standard": "^14.1.0",
    "standard-version": "^7.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "> 0.2%"
  ]
}
